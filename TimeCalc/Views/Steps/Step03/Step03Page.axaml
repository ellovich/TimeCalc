                            <TextBlock Grid.Column="1" Grid.Row="1" Margin="10" TextWrapping="Wrap" Text="Кол-во аварийных контуров АК:"/>
                            <NumericUpDown Grid.Column="2" Grid.Row="1" Classes="count" VerticalAlignment="Center" Value="{Binding Step.N_АК }"/>

                            <TextBlock Grid.Column="0" Grid.Row="3" Margin="10" TextWrapping="Wrap" Text="k_нов"/>
                            <TextBlock Grid.Column="1" Grid.Row="3" Margin="10" TextWrapping="Wrap" Text="Коэффициент новизны работы:"/>
                            <ComboBox Grid.Column="2" Grid.Row="3"
                                      VerticalAlignment="Center"
                                      Classes="correction" Items="{x:Static timeCalc:Step03.s_Corrections3_6}"
                                      SelectedItem="{Binding Step.K_нов_25}">
                                <ComboBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Name}" ToolTip.Tip="{Binding Coef}"/>
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>
                        </Grid>
                    </Border>



                </StackPanel>

            </TabItem>

        </TabControl>

        <StackPanel DockPanel.Dock="Bottom" 
                    HorizontalAlignment="Center" VerticalAlignment="Bottom"
                    Margin="20">
            <Button Classes="cancel" Command="{Binding DeleteStepCmd}" Content="Удалить этап"/>
        </StackPanel>

    </DockPanel>
</UserControl>
